{
  "name": "prerender",
  "version": "1.0.0",
  "description": "Prerendering scripts for AUIG",
  "main": "server.js",
  "scripts": {
    "start-server": "node server.js",
    "prerender": "node prerender.js",
    "prerender-all": "concurrently \"npm run start-server\" \"wait-on http://localhost:3000 && npm run prerender\"",
    "build": "node generateWIP.js",
    "serve-dist": "http-server ./dist -p 8080",
    "clean": "rimraf dist",
    "rebuild": "npm run clean && npm run build"
  },

  "dependencies": {
    "cheerio": "^1.0.0",
    "express": "^4.18.2",
    "puppeteer": "^22.8.2"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "wait-on": "^7.0.1"
  }
}
