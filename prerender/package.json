{
  "name": "prerender",
  "version": "1.0.0",
  "description": "Prerendering scripts for AUIG",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "generatePages": "node generate.js",
    "articles": "node buildArticlesPage.js",
    "build": "run-s generatePages articles",
    "serve-dist": "http-server ./pages -p 8080",
    "clean": "rimraf pages",
    "rebuild": "npm run clean && npm run build:full",
    "watch": "chokidar '../components/**/*, ../scripts/**/*, ../images/**/*, ../styles/**/*' -c 'npm run build'",
    "dev": "concurrently \"npm run watch\" \"npm run serve-dist\""
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "express": "^4.18.2",
    "puppeteer": "^22.8.2"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "wait-on": "^7.0.1",
    "chokidar-cli": "^3.0.0",
    "rimraf": "^5.0.1",
    "http-server": "^14.1.1"
  }
}
